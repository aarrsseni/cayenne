<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="rootPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.apache.cayenne.modeler.controller.MainController">
   <bottom>
      <Pane fx:id="statusBarPane" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar fx:id="menuBar" />
            <Pane fx:id="toolBarPane" />
         </children>
      </VBox>
   </top>
   <center>
      <SplitPane dividerPositions="0.2" BorderPane.alignment="CENTER">
         <items>
            <Pane fx:id="treeViewPane" minWidth="150.0" />
            <Pane fx:id="projectViewPane" minWidth="300.0" />
         </items>
      </SplitPane>
   </center>
</BorderPane>
